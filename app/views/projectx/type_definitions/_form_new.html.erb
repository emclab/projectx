<%= simple_form_for type_definition do |f| %>    
	  <%= f.input :name, :label => 'Project Name:' %>  
	  <%= f.input :for_what, :label => 'Definition For:' %>
	  <%= f.input :brief_note, :label => 'Brief Notes:' %>
	  <%= f.input :ranking_order, :label => 'Ranking Order:' %>
	  <%= f.input :active, :label => 'Active:' %>
	  
	  <%= f.button :submit, 'Save' %>  
<% end %>  


