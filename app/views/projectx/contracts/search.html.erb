<div id="main_body">
	<h1>Search</h1>
	
	<%= simple_form_for @contract,  :method => :put, :url => search_results_contracts_path  do |f| %>
	  <%= f.input :project_id, :label => "Project Id" %>
	  <%= f.input :paid_out, :label => "Paid out?" %>
	  <%= f.input :signed, :label => "Signed?" %>
	  <%= f.input :signed_by, :label => 'Signed By:', :collection => Authentify::UsersHelper.return_users('create', 'projectx_contracts') %>        

	  <%= f.button :submit, "Submit" %>
	<% end %> 
	<%= link_to 'Back', SUBURI + "/authentify/view_handler?index=0" %>

</div>
