
  <h1>Task Status</h1>
  <table>
    <tr>
      <th>#</th>
      <th>Create Date</th>
      <th>TaskStatus</th>
      <th>Brief Note</th>
      <th>Ranking Order</th>
      <th>Entered By</th>
      <th></th>
    </tr>

    <% @misc_definitions.each do |qs| %>
        <tr>
          <td><%= qs.id %></td>
          <td><%= (qs.created_at + 8.hours).strftime("%Y/%m/%d")%></td>
          <td><%= qs.name %></td>
          <td><%= qs.brief_note %></td>
          <td><%= qs.ranking_order %></td>
          <td><%= qs.last_updated_by.name %></td>
          <td><%= link_to 'Edit', edit_misc_definition_path(qs, :subaction => 'task_status', :format => :js), 
                  :remote => true if has_action_right?('update_task_status', 'projectx_misc_definitions', qs) %></td>
        </tr>
    <% end %>

  </table>

  <%= link_to 'Back', SUBURI + "/authentify/view_handler?index=0", :id => 'close' %>
  <%= link_to "New Task Status", new_misc_definition_path(:for_which => 'task_status', :subaction => 'task_status', :format => :js), :remote => true, 
             :id => 'new_log_link' if has_action_right?('create_task_status', 'projectx_misc_definitions') %>


