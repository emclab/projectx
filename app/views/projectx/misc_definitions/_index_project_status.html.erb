
  <h4><%= t(@title) %></h4>
  <table class="table table-striped">
    <tr>
      <th>#</th>
      <th><%= t('Create Date') %></th>
      <th><%= t('Status Name') %></th>
      <th><%= t('Brief Note') %></th>
      <th><%= t('Ranking Order') %></th>
      <th><%= t('Entered By') %></th>
      <th></th>
    </tr>

    <% @misc_definitions.each do |cate| %>
        <tr>
          <td><%= cate.id %></td>
          <td><%= (cate.created_at + 8.hours).strftime("%Y/%m/%d")%></td>
          <td><%= cate.name %></td>
          <td><%= cate.brief_note %></td>
          <td><%= cate.ranking_order %></td>
          <td><%= cate.last_updated_by.name %></td>
          <td><%= link_to t('Edit'), edit_misc_definition_path(cate, :subaction => 'project_status', :format => :js), 
                           :remote => true, :class =>'btn btn-primary' if has_action_right?('update_project_status', 'projectx_misc_definitions', cate) %></td>
        </tr>
    <% end %>

  </table>

	<table class="table table-striped">
	  <tr>
	    <th>
	    	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			<%= link_to t('New Project Status'), new_misc_definition_path(:for_which => 'project_status', :subaction => 'project_status', :format => :js), :remote => true, 
               :id => 'new_log_link', :class =>'btn btn-primary' if has_action_right?('create_project_status', 'projectx_misc_definitions') %>   
		</th>
	  </tr>
	</table>
  <%= link_to t('Back'), SUBURI + "/authentify/view_handler?index=0", :id => 'close' %>
