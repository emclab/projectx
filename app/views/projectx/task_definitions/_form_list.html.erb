    <table class="table table-striped">
		<tr>
			<th>#</th>
			<th><%= t('Name') %></th>
			<th><%= t('Description') %></th>
			<th><%= t('Instructions') %></th>
			<th><%= t('Active') %></th>
			<th><%= t('Last Updated By') %></th>
			<th><%= t('Ranking') %></th>
			<th></th>
		</tr>
		<% @task_definitions.each do |defi| %>
		  <tr>
		  	<td><%= defi.id %></td>
			<td><%= defi.name %></td>
			<td><%= prt(defi, :task_desp) %></td>
			<td><%= prt(defi, :task_instruction) %></td>
			<td><%= t(prt(defi, :active).to_s) %></td>
			<td><%= prt(defi, 'last_updated_by.name') %></td>    
			<td><%= prt(defi, :ranking_order) %></td>
			<td><%= link_to t('Edit'), SUBURI + "/authentify/view_handler?index=1&url=#{edit_task_definition_path(defi)}", :class => BUTTONS_CLS['action']  if has_action_right?('update', 'projectx_task_definitions', defi) %></td>
		  </tr>
		<% end %>
	</table>
	
	<table class="table table-striped">
	  <tr>
	    <th>
	    	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	    	<%= link_to t('New Task Definition'), SUBURI + "/authentify/view_handler?index=1&url=#{new_task_definition_path}", :class => BUTTONS_CLS['action'] if has_action_right?('create','projectx_task_definitions') %>
	    </th>
	  </tr>
	</table>
	<%= link_to t('Back'), SUBURI + "/authentify/view_handler?index=0", :class => BUTTONS_CLS['action'] %>
